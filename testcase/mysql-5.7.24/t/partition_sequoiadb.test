CREATE TABLE t1 ( id INT, purchased DATE )
    COMMENT 'sequoiadb: { table_options: { ReplSize: -1, Compressed: false, StrictDataMode: false }, partition_options: { Partition: 1024, AutoIndexId: true, AutoSplit: true }}'
    PARTITION BY RANGE( YEAR(purchased) )
    SUBPARTITION BY HASH( TO_DAYS(purchased) )
    SUBPARTITIONS 2 (
        PARTITION p0 VALUES LESS THAN(1990)
            COMMENT 'sequoiadb: { table_options: { Partition: 1024, ReplSize: 2, AutoIndexId: false } }',
        PARTITION p1 VALUES LESS THAN(2000)
            COMMENT 'sequoiadb: { table_options: { Partition: 2048, Compressed: true } }',   
        PARTITION p2 VALUES LESS THAN MAXVALUE
            COMMENT 'sequoiadb: { table_options: { Partition: 8192, ReplSize: 3, StrictDataMode: true } }'  
    );		       
SHOW CREATE TABLE t1;
DROP TABLE t1;

